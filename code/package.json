{
  "name": "@uyun/itsm-web",
  "version": "18.17.0-beta.58",
  "private": true,
  "disableCssModules": false,
  "main": "./lib/index.js",
  "scripts": {
    "start": "everest start",
    "build:web": "everest build",
    "build:widget": "cross-env WIDGET=true everest build",
    "widget:preview": "cross-env DEV_ENV=previewWidget everest start",
    "compile:widget": "everest compile --type=umd -m -o components ./src/widgets/myticketlist/index.js",
    "build": "zx scripts/build.mjs",
    "serve": "everest serve",
    "lint-staged": "lint-staged",
    "lint-staged:es": "eslint  --ext .js,.jsx",
    "lint:fix": "eslint --fix --ext .js,.jsx src && stylelint --fix 'src/**/*.less' --syntax less",
    "lint-html": "eslint ./src --ext .js,.jsx -f html -o ./reports/lint-results.html",
    "patch": "everest patch src -o itsm-web",
    "patch:refactor": "node patch.refactor.js",
    "babel:refactor": "babel refactor --out-dir ./itsm-web/lib --extensions '.ts,.tsx' --copy-files",
    "babel:src": "babel src --out-dir ./itsm-web/src --ignore 'src/widgets' --copy-files --no-copy-ignored",
    "biz:start": "everest doc start",
    "scp": "scp -r ./build/* root@10.1.53.184:/opt/uyun/itsm/web/web",
    "module": "node module.init.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.less": "stylelint --syntax less",
    "src/**/*.{js,jsx}": "npm run lint-staged:es",
    "src/**/*.{js,json,jsx,less}": [
      "prettier --ignore-unknown --write"
    ]
  },
